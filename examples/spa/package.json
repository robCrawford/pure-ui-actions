{
  "name": "pure-ui-actions-example-spa",
  "version": "0.1.0",
  "description": "pure-ui-actions example single page app",
  "type": "module",
  "scripts": {
    "dev": "node ./scripts/dev-watch.js",
    "ts:check": "tsc --noEmit",
    "ts:build": "tsc && rm -rf ./dist && cross-env NODE_ENV=production parcel build ./index.html --public-url ./",
