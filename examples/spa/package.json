{
  "name": "jetix-example-spa",
  "version": "0.1.0",
  "description": "Jetix example single page app",
  "scripts": {
    "dev": "parcel ./index.html",
    "ts:check": "tsc --noEmit",
    "ts:build": "tsc && rm -rf ./dist && cross-env NODE_ENV=production parcel build ./index.html --public-url ./",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "Rob Crawford",
  "repository": {
    "type": "git",
    "url": "https://github.com/robCrawford/jetix"
  },
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.13.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/ui": "^2.1.8",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "jsdom": "^25.0.1",
    "parcel": "^2.13.3",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "jetix": "^0.2.3",
    "navigo": "^8.11.1"
  }
}
