    "@eslint/js": "^9.39.1",
    "@parcel/transformer-typescript-tsc": "^2.13.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/ui": "^2.1.8",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^25.0.1",
    "parcel": "^2.13.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "navigo": "^8.11.1"
